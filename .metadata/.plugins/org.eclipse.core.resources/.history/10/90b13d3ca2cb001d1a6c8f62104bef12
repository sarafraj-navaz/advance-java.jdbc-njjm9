package method1;

public class PalindromNumber {

	public static void main(String[] args) {
		for(int i=7;i<=7;i++)
		{
			int n=i;
			int returnNumber=number(i);
			System.out.println(" prime "+returnNumber);
		}

	}
	
	public static int number(int n)
	{
		for(int i=2;i<=n-1;i++)
		{
			if(n%i==0)
			{
				System.out.println("prime numbe  "+n);
				
				//return n;
			}
			else
			{
				System.out.println("prime number  "+n);
				//return n;
			}
			
		}
		return n;
	}

}
