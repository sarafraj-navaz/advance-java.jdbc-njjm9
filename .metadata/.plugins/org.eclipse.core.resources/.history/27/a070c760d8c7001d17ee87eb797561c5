package Exercise;

public class Program18Even {
//18.WAP TO PRINT EVEN DIGITS IN A NUMBER.
	public static void main(String[] args) {
		int  n=23;
		int rem=0;
		while(n>0)
		{
		  rem=n%10;
		  int n1=rem;
		  System.out.println(n1);
		  if(n1%2==0)
		  {
			  System.err.println("this number  is Even in the  number"+n1);
		  }
		  else
		  {
			  System.err.println("odd     "+n1);
		  }
		  n=n/10;
		}

	}

}
