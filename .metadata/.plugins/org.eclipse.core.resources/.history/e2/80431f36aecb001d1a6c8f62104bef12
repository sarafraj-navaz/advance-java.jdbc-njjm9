package method1;

public class CalculatePower31 {
//31.WAP TO CALCULATE POWER OF A NUMBER.->IN CLASS
	public static void main(String[] args) {
		
		for(int i=1;i<=10;i++)
		{
			int n=power(i);
			System.out.println("square of the number "+n);
		}

	}
	public static int power(int n)
	{
		int power=n*n;
		return  power;
	}

}
